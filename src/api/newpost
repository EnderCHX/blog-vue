import global from "@/config/global.js";

/*
*
* data = {
*   title: string,
*   content: string
* }
*
* */
const NewPost = async (data) => {
    let url = global.blogApiUrl + "/newpost"
    let req = await fetch(url, {
        method: "POST",
        headers: {
            "Authorization": `Bearer `,
            "ContentType": "application/json"
        },
        body: JSON.stringify(data)
    })
}
